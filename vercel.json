{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "functions": {
      "api/*": {
        "maxDuration": 30,
        "memory": 512       
      }
    },
    "headers": [
      {
        "source": "/api/:path*",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Cache-Control",
            "value": "s-maxage=3600, stale-while-revalidate"
          }
        ]
      }
    ]
  }
  